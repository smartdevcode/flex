{
    "name": "symfony/symfony-start-composer",
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien.potencier@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../../fabpot/dotenv"
        }
    ],
    "require": {
        "composer-plugin-api": "^1.0",
        "fabpot/dotenv": "@dev"
    },
    "require-dev": {
        "composer/composer": "^1.0.0"
    },
    "autoload": {
        "classmap": [
            "SymfonyStartPlugin.php",
            "PackageConfigurator.php"
        ],
        "psr-4": {
            "Symfony\\Start\\": "recipes"
        }
    },
    "extra": {
        "class": "Symfony\\Start\\ComposerPlugin"
    }
}
